{
  "name": "xhr-fetch",
  "version": "0.0.1",
  "description": "Async/await ajax APIs on top of axios",
  "main": "dist/xhr-fetch.js",
  "types": "dist/xhr-fetch.d.ts",
  "scripts": {
    "build": "npm run clean && tsc src/xhr-fetch.ts --outDir ./dist --sourceMap --target ES5 --lib ES2015 --declaration",
    "dev": "tsc",
    "clean": "rm -rf ./build ./dist",
    "test": "npm run dev && ava build/*.spec.js",
    "format": "prettier --single-quote --no-semi --jsx-bracket-same-line --write src/*.{js,ts}",
    "watch": "chokidar 'src/**/*.*' -c 'pm run test'"
  },
  "keywords": [],
  "author": "Tan Nhu",
  "license": "MIT",
  "dependencies": {
    "axios": "0.16.2"
  },
  "devDependencies": {
    "@types/node": "latest",
    "ava": "^0.22.0",
    "chokidar-cli": "latest",
    "prettier": "^1.5.3",
    "typescript": "latest"
  }
}
